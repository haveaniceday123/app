<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".splash.StartActivity" android:background="#FDFCFD">

    <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/lottie_location"
            android:layout_width="300dp" android:layout_height="300dp"
            app:lottie_fileName="logo.json"
            app:lottie_autoPlay="true"
            app:lottie_repeatCount="0"
            android:layout_centerHorizontal="true" android:layout_marginTop="175dp"
            android:layout_alignParentTop="true"/>

    <ImageView
            android:src="@drawable/yanolza"
            android:layout_width="344dp"
            android:layout_height="69dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="35dp"
            android:id="@+id/imageView"
            android:layout_centerHorizontal="true"/>


</RelativeLayout>